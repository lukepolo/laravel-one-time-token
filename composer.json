{
  "name": "lukepolo/laravel-passport-one-time-token",
  "description": "Gives the ability to create one time tokens easily with Laravel Passport",
  "keywords": [
    "laravel",
    "passport",
    "one time token"
  ],
  "homepage": "https://github.com/lukepolo/laravel-passport-one-time-token",
  "license": "MIT",
  "authors": [
    {
      "name": "Luke Policinski",
      "email": "Luke@LukePOLO.com",
      "homepage": "http://LukePOLO.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.4.0",
    "laravel/Passport": "^3.0.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "~7.5"
  },
  "autoload": {
    "psr-4": {
      "LukePOLO\\LaravelPassportOneTimeToken\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LukePOLO\\LaravelPassportOneTimeToken\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "LukePOLO\\LaravelPassportOneTimeToken\\ServiceProvider"
      ]
    }
  },
  "prefer-stable" : true,
  "minimum-stability": "dev"
}